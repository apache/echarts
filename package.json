    "./dist/echarts.common": "./dist/echarts.common.js",
    "./dist/echarts.common.min": "./dist/echarts.common.min.js",
    "./dist/echarts.esm": "./dist/echarts.esm.mjs",
    "./dist/echarts.esm.min": "./dist/echarts.esm.min.mjs",
